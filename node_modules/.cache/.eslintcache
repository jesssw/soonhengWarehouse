) => {\r\n    const { name, value } = event.target;\r\n    if (name === \"category\") {\r\n      setSelectedCategory(value);\r\n    } \r\n    // else if (name === \"priceRange\") {\r\n    //   setSelectedPriceRange(value);\r\n    // }\r\n     else if (name === \"model\") {\r\n      setSelectedModel(value);\r\n    }\r\n    else if (name === \"company\") {\r\n    setSelectedCompany(value);\r\n  }\r\n  };\r\n\r\n  const resetPage = () => {\r\n    setCurrentPage(1); // Reset the currentPage state variable to 1\r\n  };\r\n  \r\n  function filteredData(products, selectedCategory, selectedModel, selectedCompany, query) {\r\n    let filteredProducts = [...products]; // Make a copy of the original products array\r\n\r\n    if (selectedCategory) {\r\n      filteredProducts = filteredProducts.filter(product =>\r\n        product.category === selectedCategory\r\n      );\r\n    }\r\n\r\n\r\n    if (selectedModel) {\r\n      filteredProducts = filteredProducts.filter(product =>\r\n        product.model === selectedModel\r\n      );\r\n    }\r\n\r\n    if (selectedCompany) {\r\n      filteredProducts = filteredProducts.filter(product =>\r\n        product.company === selectedCompany\r\n      );\r\n    }\r\n\r\n    if (query) {\r\n      filteredProducts = filteredProducts.filter(product =>\r\n        product.title.toLowerCase().includes(query.toLowerCase())//search by title\r\n       || String(product.barcode_id).toLowerCase().includes(query.toLowerCase()) //search by barcode\r\n\r\n\r\n      );\r\n    }\r\n\r\n    return filteredProducts;\r\n  }\r\n\r\n  const result = filteredData(products, selectedCategory, selectedModel, selectedCompany, query);\r\n  const totalItems = result.length;\r\n\r\n  const indexOfLastItem = currentPage * itemsPerPage;\r\n  const indexOfFirstItem = indexOfLastItem - itemsPerPage;\r\n  const currentItems = result.slice(indexOfFirstItem, indexOfLastItem);\r\n\r\n  const paginate = pageNumber => setCurrentPage(pageNumber);\r\n\r\n  // console.log(\"Result:\", result); // Debugging: Log result before rendering\r\n  // console.log(\"Total items:\", result.length);\r\n\r\n  return (\r\n    <>\r\n    <div class=\"filter-header\">\r\n    <Navigation query={query} handleInputChange={handleInputChange} />\r\n      <Sidebar handleChange={handleChange} selectedCompany={selectedCompany} resetPage={resetPage} />\r\n    </div>\r\n    \r\n\r\n      {/* <Recommended handleClearFilters={handleClearFilters}  handleClick={handleClick} /> */}\r\n      <div class=\"content-wrapper\">\r\n        <Products\r\n          selectedCategory={selectedCategory}\r\n          selectedCompany={selectedCompany}\r\n          selectedModel={selectedModel}\r\n          result={currentItems}\r\n        />\r\n\r\n      </div>\r\n      <Pagination currentPage={currentPage} itemsPerPage={itemsPerPage} totalItems={totalItems} paginate={paginate} />\r\n\r\n\r\n\r\n    </>\r\n  );\r\n}\r\n\r\n\r\nexport default App;\r\n","C:\\xampp\\htdocs\\sh-warehouse\\soonhengWarehouse\\src\\Navigation\\Nav.js",[],[],"C:\\xampp\\htdocs\\sh-warehouse\\soonhengWarehouse\\src\\Products\\Products.js",[],[],"C:\\xampp\\htdocs\\sh-warehouse\\soonhengWarehouse\\src\\Recommended\\Recommended.js",[],[],"C:\\xampp\\htdocs\\sh-warehouse\\soonhengWarehouse\\src\\db\\data.js",[],[],"C:\\xampp\\htdocs\\sh-warehouse\\soonhengWarehouse\\src\\components\\Card.js",[],[],"C:\\xampp\\htdocs\\sh-warehouse\\soonhengWarehouse\\src\\Sidebar\\Sidebar.js",[],[],"C:\\xampp\\htdocs\\sh-warehouse\\soonhengWarehouse\\src\\components\\Button.js",[],[],"C:\\xampp\\htdocs\\sh-warehouse\\soonhengWarehouse\\src\\Sidebar\\Category\\Category.js",[],[],"C:\\xampp\\htdocs\\sh-warehouse\\soonhengWarehouse\\src\\components\\Input.js",[],[],[],"C:\\xampp\\htdocs\\sh-warehouse\\soonhengWarehouse\\src\\Sidebar\\Company\\Company.js",[],[],"C:\\xampp\\htdocs\\sh-warehouse\\soonhengWarehouse\\src\\Sidebar\\Model\\Model.js",[],[],"C:\\xampp\\htdocs\\sh-warehouse\\soonhengWarehouse\\src\\Pagination\\Pagination.js",[],[